{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>gopad/.github//renovate/preset"
  ],
  "packageRules": [
    {
      "matchManagers": [
        "gomod"
      ],
      "matchDepTypes": [
        "golang"
      ],
      "enabled": false
    },
    {
      "description": "Ignore broken updates",
      "matchPackageNames": [
        "eslint"
      ],
      "enabled": false
    }
  ]
}
